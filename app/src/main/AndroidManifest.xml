<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.simanja">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Simanja">
        <activity
            android:name=".Navigation_Drawer"
            android:label="@string/title_activity_navigation_drawer"
            android:theme="@style/Theme.Simanja.NoActionBar"></activity>
        <activity
            android:name=".ProfilePage"
            android:label="@string/title_activity_profile_page"
            android:theme="@style/Theme.Simanja.NoActionBar" />
        <activity
            android:name=".Home_Page"
            android:label="@string/title_activity_home_page"
            android:theme="@style/Theme.Simanja.NoActionBar" />
        <activity
            android:name=".LandingPage"
            android:label="@string/title_activity_landing_page"
            android:theme="@style/Theme.Simanja.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>